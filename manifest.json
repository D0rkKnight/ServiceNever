{
    "manifest_version": 3,
    "name": "ServiceNever",
    "version": "1.0",
    "description": "I am getting paid for this",
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [{
      "matches": ["https://*/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }],
    "background": {
      "service_worker": "bg_bundle.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["format.json"],
        "matches": ["https://*/*"]
      }
    ],
    "action": {
      "default_title": "Click Me",
      "default_popup": "popup.html"
    }
}